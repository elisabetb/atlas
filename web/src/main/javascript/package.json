{
  "name": "expression-atlas-webpack-bundles",
  "version": "1.0.0",
  "description": "Expression Atlas JavaScript Webpack bundles ",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "rm -rf ../webapp/resources/js-bundles/* && webpack --progress --colors --watch --debug --output-pathinfo --display-chunks --display-modules --devtool source-map",
    "dev-min": "rm -rf ../webapp/resources/js-bundles/* && webpack --progress --colors --optimize-minimize --output-pathinfo --display-chunks --display-modules",
    "ci": "rm -rf ../webapp/resources/js-bundles/* && webpack --debug --output-pathinfo --display-chunks --display-modules --devtool source-map",
    "prod": "rm -rf ../webapp/resources/js-bundles/* && webpack --debug --optimize-minimize --output-pathinfo --display-chunks --display-modules"
  },
  "author": "Elisabet Barrera <barrera@ebi.ac.uk> and Alfonso Mu√±oz-Pomer <amunoz@ebi.ac.uk>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-react-jsx": "^6.6.5",
    "chai": "^3.0.0",
    "clean-webpack-plugin": "^0.1.8",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^1.6.3",
    "imports-loader": "^0.6.5",
    "mocha": "^2.0.1",
    "mocha-loader": "^0.7.1",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.2.8",
    "style-loader": "^0.13.0",
    "testdom": "^2.0.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "eve": "^0.4.2",
    "jquery": "^2.1.4",
    "jquery-hc-sticky": "git+https://github.com/alfonsomunozpomer/hc-sticky.git",
    "jquery-ui-bundle": "^1.11.4",
    "jquery.browser": "0.0.8",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "urijs": "^1.16.0"
  }
}
